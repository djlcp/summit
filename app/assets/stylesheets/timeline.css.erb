.timeline_div {
	display: flex;
	margin-left: 20%;
	margin-right: 20%;
}

.timeline_line {
	align-self: center;
	width: 100%;
	height: 5px;
	background: grey;
	margin: 15px;
	display: flex;
}

.timeline_item {
	z-index: 1;
	flex-direction: row;
	position: relative;
}

.dot {
	height: 20px;
	width: 20px;
	border-radius: 50%;
	background: green;
	top: -7.5px;
	left: -20px;
	margin-right: -20px;
}